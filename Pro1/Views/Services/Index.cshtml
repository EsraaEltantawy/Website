@model IEnumerable<Pro1.Models.Service>



<section class="page-section" id="services">
    <div class="container">
        <div class="text-center">
            <h2 class="section-heading text-uppercase">Services</h2>
            <h3 class="section-subheading text-muted">Lorem ipsum dolor sit amet consectetur.</h3>
        </div>
        <div class="row text-center">
    @foreach (var item in Model)
    {

        <div class="col-md-4">
            <span class="@item.sr_Class"><i class="fas fa-circle fa-stack-2x text-primary"></i><i class="fas fa-shopping-cart fa-stack-1x fa-inverse"></i></span>
            <h4 class="my-3">@item.sr_Name</h4>
            <p class="text-muted">@item.sr_Dis</p>
        </div>

    }
       </div>
    </div>
</section>